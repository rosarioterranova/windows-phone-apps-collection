                        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="contattami" VerticalAlignment="Center" FontSize="36" Foreground="Black" Margin="15,0,0,0"/>
                    </StackPanel>
                    <StackPanel x:Name="store" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="415" Orientation="Horizontal" Margin="0,15,0,0" Tap="store_Tap">
