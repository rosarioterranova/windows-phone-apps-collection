                        <Grid HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="100">
                            <Grid.Background>
                                <ImageBrush ImageSource="/images/sfoglia.png"/>
                            </Grid.Background>
                        </Grid>
                        <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="sfoglia le fiabe" VerticalAlignment="Center" FontSize="36" Foreground="Black" Margin="15,0,0,0"/>
                    </StackPanel>
                    <StackPanel x:Name="vota" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="415" Orientation="Horizontal" Margin="0,15,0,0" Tap="vota_Tap">
