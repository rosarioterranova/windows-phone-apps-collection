    <!--LayoutRoot è la griglia radice in cui viene inserito tutto il contenuto della pagina-->
    <Grid x:Name="LayoutRoot">
        <Grid.Background>
            <ImageBrush Stretch="Fill" ImageSource="/quaderna.jpg"/>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <!--TitlePanel contiene il nome dell'applicazione e il titolo della pagina-->
        <StackPanel x:Name="TitlePanel" Grid.Row="0" Margin="12,17,0,28">
            <TextBlock x:Name="PageTitle" Margin="0,-7,10,0" Style="{StaticResource PhoneTextTitle1Style}" Foreground="Black">
				<Run Text="Impossible Tris"/>
            </TextBlock>
        </StackPanel>

        <!--ContentPanel - inserire ulteriore contenuto qui-->
        <Grid x:Name="ContentPanel" Grid.Row="1" Margin="12,0,12,0">
            <Button x:Name="singleButton" Content="Giocatore singolo" HorizontalAlignment="Left" Margin="93,205,0,0" VerticalAlignment="Top" BorderBrush="Black" Foreground="Black" Click="singleButton_Click" Width="253"/>
            <Button x:Name="multiButton" Content="Due giocatori" HorizontalAlignment="Left" Margin="93,365,0,0" VerticalAlignment="Top" Width="253" BorderBrush="Black" Foreground="Black" Click="multiButton_Click"/>
            <Button x:Name="infoButton" Content="Informazioni" HorizontalAlignment="Left" Margin="93,445,0,0" VerticalAlignment="Top" Width="253" BorderBrush="Black" Foreground="Black" Click="infoButton_Click"/>
            <Image Margin="116,-32,114,440" Source="/logo.png" Stretch="Fill"/>
            <Button x:Name="facileButton" Content="Modalità facile" HorizontalAlignment="Left" Margin="93,285,0,0" VerticalAlignment="Top" Width="253" BorderBrush="Black" Foreground="Black" Click="facileButton_Click"/>
        </Grid>
